<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_Format_Check</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(CONTAINS(Email__c, &quot;@gmail.com&quot;))</errorConditionFormula>
    <errorDisplayField>Email__c</errorDisplayField>
    <errorMessage>Only Gmail addresses are allowed</errorMessage>
</ValidationRule>
